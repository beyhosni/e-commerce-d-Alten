# Configuration du serveur
SERVER_PORT=8080

# Configuration de la base de donn√©es H2
DB_URL=jdbc:h2:mem:ecommerce
DB_DRIVER=org.h2.Driver
DB_USERNAME=sa
DB_PASSWORD=password

# Configuration de la console H2
H2_CONSOLE_ENABLED=true
H2_CONSOLE_PATH=/h2-console

# Configuration JPA/Hibernate
JPA_DATABASE_PLATFORM=org.hibernate.dialect.H2Dialect
JPA_HIBERNATE_DDL_AUTO=update
JPA_SHOW_SQL=false

# Configuration JWT
JWT_SECRET=mySecretKey
JWT_EXPIRATION=86400000

# Configuration Swagger/OpenAPI
SPRINGDOC_API_DOCS_PATH=/api-docs
SPRINGDOC_SWAGGER_UI_PATH=/swagger-ui.html

# Configuration du logging
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_ALTEN_ECOMMERCE=DEBUG
